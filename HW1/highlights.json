{
  "scopeName": "source.abc",
  "patterns": [{ "include": "#expression" }],
  "repository": {
    "expression": {
      "patterns": [{ "include": "#kw" }, { "include": "#constructor" }, { "include": "#olv" }, { "include": "#numbers" }, { "include": "#parantheses" }]
    },
    "kw": {
      "match": "\\b(fresh|in|import|run)\\b",
      "name": "keyword.control"
    },
    "constructor": {
      "match": "\\b([A-Z][_0-9A-Za-z]*)\\b",
      "name": "constant.character"
    },
    "olv": {
      "match": "\\b([a-zA-z])\\b",
      "name": "keyword.onelettervariables"
    },
    "parantheses": {
      "match": "\\b([\\(\\)]*)\\b",
      "name": "keyword.control"
    },
    "numbers": {
      "match": "\\b([0-9]*)\\b",
      "name": "constant.character"
    }
  }
}
