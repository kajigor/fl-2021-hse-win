
{
	"scopeName": "source.fl1",
	"patterns": [{ "include": "#expression" }],
	"repository": {
	  "expression": {
		"patterns": [{ "include": "#kw" }, { "include": "#vertex" }]
	  },
	  "kw": {
		"match": "\\b(with|alphabet|terminal)\\b",
		"name": "keyword.control"
	  },
	  "vertex": {
		"match": "\\b(q[0-9]*)\\b",
		"name": "entity.name.type.scope"
	  }
	}
  }